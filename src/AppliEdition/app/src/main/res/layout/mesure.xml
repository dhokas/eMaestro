<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:id="@+id/m"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/mesure" />

    <ImageView
        android:id="@+id/diese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/diese_edition" />
    <ImageView
        android:id="@+id/bemol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/bemol_edition" />
    <ImageView
        android:id="@+id/alerte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tempoModif"
        android:layout_toRightOf="@+id/bemol"
        android:background="@drawable/alerte" />

    <ImageView
        android:id="@+id/PrePassage"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/prem_passage"
        android:alpha=".0"/>
    <ImageView
        android:id="@+id/SecPassage"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/sec_passage"
        android:alpha=".0"/>
    <ImageView
        android:id="@+id/debutReprise"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/debut_rep"
        android:alpha=".0"/>

    <ImageView
        android:id="@+id/debutPassage"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/debut_passage"
        android:alpha=".0"/>
    <ImageView
        android:id="@+id/barrePassage"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/barre_passage"
        android:alpha=".0"/>

    <ImageView
        android:id="@+id/finPassage"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/fin_passage"
        android:alpha=".0"/>
    <ImageView
        android:id="@+id/finReprise"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_below="@+id/tempoModif"
        android:background="@drawable/fin_reprise"
        android:alpha=".0"/>

    <ImageView
        android:id="@+id/nuance"
        android:layout_width="80dp"
        android:layout_height="38dp"
        android:layout_marginTop="12dp"
        android:alpha=".75"
        android:layout_below="@+id/tempoModif"
        android:background="@color/neutre"/>

    <TextView
        android:id="@+id/id"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:layout_below="@+id/m"
        android:layout_centerInParent="true"
        android:layout_marginLeft="40dp"

        />

    <TextView
        android:id="@+id/tempoModif"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:textSize="20px"
        android:textAlignment="center"
        android:text="1"


        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>